NoConcatenate

[T_Appjus_Completo]:

LOAD   
  	idlogaplicacao,
    cpf,
    datahorasolicitacao,
    datahoraretorno,
    operacao,
    sucesso,
    msg,
    ip,
    porta,
    so,
    versaoso,
    nomedispositivo,
    "useragent",
    pergunta,
  	"Número de Processo",
    "Unidade Julgadora ID",     
    "ID Conclusão",
    "Comarca Projudi", 
    "Vara Projudi",
    "Nível de Sigilo",
    "Número Único",
    if("Nível de Sigilo" = 'Público', login, 'Login' & AutoNumber(idlogaplicacao)) as login,
    if("Nível de Sigilo" = 'Público', Magistrado, 'Magistrado' & AutoNumber(idlogaplicacao)) as Magistrado
    
RESIDENT T_Appjus_1G;

//--------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Concatenate(T_Appjus_Completo)

LOAD   
  	idlogaplicacao,
    cpf,
    datahorasolicitacao,
    datahoraretorno,
    operacao,
    sucesso,
    msg,
    ip,
    porta,
    so,
    versaoso,
    nomedispositivo,
    "useragent",
    pergunta,
  	"Número de Processo",
    "Unidade Julgadora ID",     
    "ID Conclusão",
    "Comarca Projudi", 
    "Vara Projudi",
    "Nível de Sigilo",
    "Número Único",
    if("Nível de Sigilo" = 'Público', login, 'Login' & AutoNumber(idlogaplicacao)) as login,
    if("Nível de Sigilo" = 'Público', Magistrado, 'Magistrado' & AutoNumber(idlogaplicacao)) as Magistrado

RESIDENT T_Appjus_Conclusoes_2G_Sessao_Turma;

//--------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Concatenate(T_Appjus_Completo)

LOAD   
  	idlogaplicacao,
    cpf,
    datahorasolicitacao,
    datahoraretorno,
    operacao,
    sucesso,
    msg,
    ip,
    porta,
    so,
    versaoso,
    nomedispositivo,
    "useragent",
    pergunta,
  	"Número de Processo",
    "Unidade Julgadora ID",     
    "ID Conclusão",
    "Comarca Projudi", 
    "Vara Projudi",
    "Nível de Sigilo",
    "Número Único",
    if("Nível de Sigilo" = 'Público', login, 'Login' & AutoNumber(idlogaplicacao)) as login,
    if("Nível de Sigilo" = 'Público', Magistrado, 'Magistrado' & AutoNumber(idlogaplicacao)) as Magistrado

RESIDENT T_Appjus_Conclusoes_2G_Sessao_Digital;

//--------------------------------------------------------------------------------------------------------------------------------------------------------------------//

DROP TABLE T_Appjus_1G;
DROP TABLE T_Appjus_Conclusoes_2G_Sessao_Turma;
DROP TABLE T_Appjus_Conclusoes_2G_Sessao_Digital;

STORE T_Appjus_Completo INTO [lib://Corporativo/Judicial/MonitoramentoAppTJPRJus/Transformacao/T_Appjus.qvd] (qvd);

DROP TABLE T_Appjus_Completo;
