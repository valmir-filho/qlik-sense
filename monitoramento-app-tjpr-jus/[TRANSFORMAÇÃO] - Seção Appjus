FROM [lib://Corporativo/Judicial/Geral/T_BaseVarasProjudi.qvd] (qvd);

//--------------------------------------------------------------------------------------------------------------------------------------------------------------------//
 
LEFT JOIN(T_Appjus_1G)

LOAD 
    "Unidade Julgadora ID", 
    "Comarca Projudi", 
    "Vara Projudi"

Resident T_Varas;
 
